<div class="container">
  <table class="table">
    <thead>
      <tr>
      {{#each products.headers}}
        <th scope="col">{{this}}</th>
      {{/each}}
      </tr>
    </thead>
    <tbody>
      {{#each products.data}}
      <tr>
        {{#with this}}
          <td>{{ProductID}}</td>
          <td>{{Name}}</td>
          <td>{{Color}}</td>
          <td>{{StandardCost}}</td>
          <td>{{ListPrice}}</td>
          <td>{{Size}}</td>
          <td>{{Weight}}</td>
          <td>{{DaysToManufacture}}</td>
        {{/with}}
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>
